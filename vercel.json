{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "owlcloud",
  "rewrites": [
    {
      "source": "/extension/(.*)",
      "destination": "/owlbear-extension/$1"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/extension/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
